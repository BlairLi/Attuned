{
  "expo": {
    "name": "attuned-dev",
    "slug": "attuned-dev",
    "version": "1.0.0",
    "scheme": "attuned-dev",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-av",
        {
          "microphone": "We need access to your microphone to detect notes"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.attuneddev"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.attuneddev"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
